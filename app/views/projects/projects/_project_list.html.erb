<div class="table-body" style="border: thin solid;">
  <div class="project-name" style="border: thin solid;">
    <%= link_to "#{project.project_name}", user_project_path(@user, project.id), class: "project-detail-link" %>
  </div>
  <div class="project-leader" style="border: thin solid;">
    <%= User.find(project.project_leader_id).user_name %>
  </div>
  <div class="next-report-date" style="border: thin solid;">
    <%= l(project.project_next_report_date, format: :long) %>
  </div>
  <%#= link_to "詳細", user_project_path(@user, project.id), class: "btn btn-primary btn-sm" %>
  <%#= link_to "編集", edit_user_project_path(@user, project.id), remote: true, class: "btn btn-primary btn-sm", data: {"turbolinks" => false} %>
  <%#= link_to "削除", user_project_path(@user, project.id), method: :delete, data: { confirm: "#{project.project_name}を削除してよろしいですか？" }, class: "btn btn-danger btn-sm" %>
</div>
