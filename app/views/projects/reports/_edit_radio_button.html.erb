<div class="container">
<%= RadioButton.find(pattern.question_id).label_name %>
<br>
<% RadioButton.find(pattern.question_id).radio_button_option_strings.each do |item| %>
  <%= f.fields_for "answer[#{c}]", mo do |m| %>
    <%= m.radio_button :value, item.option_string, required: true %>
    <%= item.option_string %>
    <br>
  <% end %>
<% end %>
</div>