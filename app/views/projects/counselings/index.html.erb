<span>▼あなた宛の相談一覧▼</span>
    <table>
      <thead>
        <th>概要</th>
        <th>連絡者</th>
      </thead>

      <tbody>
        <% @counselings.each do |counseling| %>
          <tr>
            <td>
              <%= link_to counseling.title, user_project_counseling_path(current_user,@project,counseling) %>
            </td>
            <td>
              <%= @member.find(counseling.sender_id).user_name %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <p class="center" style="padding: 10px;"><%= link_to '戻る', user_project_path(@user,@project),class: "btn btn-light btn-outline-dark col-2" %></p>